{
  "name": "@wooters/crabcallr",
  "version": "0.2.0",
  "description": "OpenClaw plugin for CrabCallr voice interface - talk to OpenClaw via phone or browser",
  "type": "module",
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "scripts": {
    "build": "tsc",
    "dev": "tsc --watch",
    "clean": "rm -rf dist",
    "lint": "eslint src/**/*.ts",
    "prepublishOnly": "npm run build"
  },
  "keywords": [
    "openclaw",
    "openclaw-plugin",
    "voice",
    "crabcallr",
    "telephony",
    "webrtc"
  ],
  "author": "Wooters",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/wooters/openclaw-plugin.git"
  },
  "homepage": "https://crabcallr.com",
  "bugs": {
    "url": "https://github.com/wooters/openclaw-plugin/issues"
  },
  "files": [
    "dist",
    "skills",
    "openclaw.plugin.json"
  ],
  "dependencies": {
    "@sinclair/typebox": "^0.32.0",
    "ws": "^8.16.0"
  },
  "devDependencies": {
    "@eslint/js": "^9.39.2",
    "@types/node": "^20.11.0",
    "@types/ws": "^8.5.10",
    "commander": "^12.0.0",
    "eslint": "^9.39.2",
    "openclaw": "2026.2.3-1",
    "typescript": "^5.3.3",
    "typescript-eslint": "^8.54.0"
  },
  "openclaw": {
    "extensions": [
      "./dist/index.js"
    ],
    "channel": {
      "id": "crabcallr",
      "label": "CrabCallr Voice",
      "selectionLabel": "CrabCallr (Voice)",
      "docsPath": "/channels/crabcallr",
      "blurb": "Voice calling via phone or browser",
      "order": 100,
      "aliases": [
        "voice",
        "phone"
      ]
    },
    "install": {
      "npmSpec": "@wooters/crabcallr",
      "defaultChoice": "npm"
    }
  },
  "engines": {
    "node": ">=18.0.0"
  }
}
